@model FreelancerProject.Models.Freelancer_Competence

@{
    ViewBag.Title = "Kompetens";
}

<h2>Ta bort kompetens</h2>


<h3><span class="fas fa-exclamation-triangle fa-spin" style="color:red"></span>  Är du säker på att du vill ta bort denna kompetens från ditt CV?</h3>

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.Competence.CompetenceName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Competence.CompetenceName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Ranking)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ranking)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Ta bort" class="btn btn-default" /> |
            @Html.ActionLink("Tillbaka till CV", "Index", "FreelancerCV")
        </div>
    }
</div>
